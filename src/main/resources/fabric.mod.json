{
	"schemaVersion": 1,
	"id": "vs-skirmish",
	"version": "${version}",
	"name": "VS Skirmish",
	"description": "Challenge other teams to a skirmish with your VS Ships.",
	"authors": [
		"Mad Mike"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/vs-skirmish/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"madmike.skirmish.VSSkirmish"
		],
		"client": [
			"madmike.VSSkirmishClient"
		],
		"cardinal-components-scoreboard": [
			"madmike.skirmish.component.SkirmishComponents"
		]
	},
	"mixins": [
		"vs-skirmish.mixins.json",
		{
			"config": "vs-skirmish.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.17.2",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	},
	"custom": {
		"cardinal-components": [
			"vs-skirmish:names"
		]
	}
}